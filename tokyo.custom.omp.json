{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#7eb8da",
          "style": "plain",
          "template": " <#ffffff></> {{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "#8800dd",
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true,
            "style": "austin",
            "threshold": 500
          },
          "style": "plain",
          "template": " <#fefefe></>{{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#906cff",
          "foreground": "#100e23",
          "properties": {
            "display_mode": "files"
          },
          "style": "plain",
          "template": "  {{ .Full }} ",
          "type": "python"
        },
        {
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": "",
            "yarn_icon": ""
          },
          "style": "plain",
          "template": "  {{ .Full }} ",
          "type": "node"
        },
        {
          "foreground": "#ffa5d8",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "[<#ffffff>{{ .UpstreamIcon }}</>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#ffffff></> {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#ffffff></> {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} <#ffffff></> {{ .StashCount }}{{ end }}]",
          "type": "git"
        },
        {
          "foreground": "#ffa5d8",
          "style": "plain",
          "template": "[]",
          "type": "root"
        },
        {
          "foreground": "#ffa5d8",
          "style": "plain",
          "template": "[<#ffffff>ﲍ</> Error, check your command]",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#FFD664",
          "foreground": "#111111",
          "style": "plain",
          "type": "upgrade"
        },
        {
          "foreground": "#7eb8da",
          "properties": {
            "time_format": "Monday at 3:04:05 PM"
          },
          "style": "diamond",
          "template": "<#ffffff></>{{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#7eb8da",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": " <#98bfad>{{ .Path }}</>",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#7eb8da",
          "style": "plain",
          "template": " $",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "error_line": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "<#eb6f92,#286983> </><#286983,transparent></> "
  },
  "final_space": true,
  "valid_line": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "<#e0def4,#286983> </><#286983,transparent></> "
  },
  "version": 2
}